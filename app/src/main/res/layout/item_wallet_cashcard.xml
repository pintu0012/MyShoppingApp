<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:id="@+id/linear_wallet_item_root" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:gravity="center_vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
        <LinearLayout android:orientation="horizontal" android:id="@+id/linear_axis_bank" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="0.9">
            <ImageView android:layout_gravity="center_vertical" android:id="@+id/img_wallet_logo" android:padding="5dp" android:layout_width="50dp" android:layout_height="50dp" android:layout_marginLeft="5dp"/>
            <TextView android:textSize="12sp" android:textColor="#000000" android:layout_gravity="center_vertical" android:id="@+id/text_wallet_pay_cash" android:padding="3dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="5dp" android:text="PayCash"/>
        </LinearLayout>
        <LinearLayout android:gravity="center" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="0.1">
            <ImageView android:id="@+id/img_select_image" android:focusable="false" android:layout_width="15dp" android:layout_height="15dp"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
