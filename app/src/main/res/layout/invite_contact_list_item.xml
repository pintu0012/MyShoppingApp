<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:padding="10dp" android:descendantFocusability="blocksDescendants" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.appvirality.appviralityui.custom.RoundedImageView android:id="@+id/iv_contact" android:layout_width="40dp" android:layout_height="40dp" android:src="@drawable/appvirality_user_image" android:layout_centerVertical="true"/>
    <TextView android:textSize="13sp" android:textColor="#606060" android:id="@+id/tv_contact" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_marginRight="10dp" android:layout_toLeftOf="@+id/cb_contact" android:layout_toRightOf="@+id/iv_contact" android:layout_centerVertical="true"/>
    <CheckBox android:id="@+id/cb_contact" android:background="@android:color/transparent" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
</RelativeLayout>
