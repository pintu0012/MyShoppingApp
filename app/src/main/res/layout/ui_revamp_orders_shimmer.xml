<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/margin_16dp"
    android:layout_marginTop="@dimen/margin_8dp"
    android:layout_marginRight="@dimen/margin_16dp"
    android:background="@drawable/ui_revamp_common_border_layout"
    android:orientation="vertical"
    android:padding="@dimen/margin_12dp">

    <View
        android:id="@+id/view1"
        android:layout_width="90dp"
        android:layout_height="@dimen/margin_10dp"
        android:background="@color/greye0e6e9"
        app:layout_constraintStart_toStartOf="0"
        app:layout_constraintTop_toTopOf="0" />

    <View
        android:layout_width="@dimen/margin_40dp"
        android:layout_height="@dimen/margin_10dp"
        android:background="@color/greye0e6e9"
        app:layout_constraintEnd_toEndOf="0"
        app:layout_constraintTop_toTopOf="0" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/margin_12dp"
        android:paddingBottom="@dimen/margin_12dp"
        app:layout_constraintTop_toBottomOf="@+id/view1">

        <View
            android:id="@+id/view2"
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_1dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintTop_toTopOf="0" />

        <View
            android:id="@+id/view3"
            android:layout_width="54dp"
            android:layout_height="54dp"
            android:layout_marginTop="@dimen/margin_12dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toStartOf="0"
            app:layout_constraintTop_toBottomOf="@+id/view2" />

        <View
            android:id="@+id/view4"
            android:layout_width="@dimen/margin_30dp"
            android:layout_height="@dimen/margin_10dp"
            android:layout_marginLeft="@dimen/size_22dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toEndOf="@+id/view3"
            app:layout_constraintTop_toTopOf="@+id/view3" />

        <View
            android:id="@+id/view5"
            android:layout_width="@dimen/size_100dp"
            android:layout_height="@dimen/margin_10dp"
            android:layout_marginTop="@dimen/margin_8dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toStartOf="@+id/view4"
            app:layout_constraintTop_toBottomOf="@+id/view4" />

        <View
            android:id="@+id/view6"
            android:layout_width="@dimen/margin_30dp"
            android:layout_height="@dimen/margin_10dp"
            android:layout_marginTop="@dimen/margin_8dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toStartOf="@+id/view5"
            app:layout_constraintTop_toBottomOf="@+id/view5" />

        <View
            android:id="@+id/view7"
            android:layout_width="@dimen/margin_30dp"
            android:layout_height="@dimen/margin_10dp"
            android:layout_marginTop="@dimen/margin_8dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toStartOf="@+id/view6"
            app:layout_constraintTop_toBottomOf="@+id/view6" />

        <View
            android:id="@+id/view8"
            android:layout_width="90dp"
            android:layout_height="@dimen/margin_10dp"
            android:layout_marginTop="@dimen/margin_8dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toStartOf="@+id/view7"
            app:layout_constraintTop_toBottomOf="@+id/view7" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/margin_12dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout1">

        <View
            android:id="@+id/view22"
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_1dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintTop_toTopOf="0" />

        <View
            android:id="@+id/view32"
            android:layout_width="54dp"
            android:layout_height="54dp"
            android:layout_marginTop="@dimen/margin_12dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toStartOf="0"
            app:layout_constraintTop_toBottomOf="@+id/view22" />

        <View
            android:id="@+id/view42"
            android:layout_width="@dimen/margin_30dp"
            android:layout_height="@dimen/margin_10dp"
            android:layout_marginLeft="@dimen/size_22dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toEndOf="@+id/view32"
            app:layout_constraintTop_toTopOf="@+id/view32" />

        <View
            android:id="@+id/view52"
            android:layout_width="@dimen/size_100dp"
            android:layout_height="@dimen/margin_10dp"
            android:layout_marginTop="@dimen/margin_8dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toStartOf="@+id/view42"
            app:layout_constraintTop_toBottomOf="@+id/view42" />

        <View
            android:id="@+id/view62"
            android:layout_width="@dimen/margin_30dp"
            android:layout_height="@dimen/margin_10dp"
            android:layout_marginTop="@dimen/margin_8dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toStartOf="@+id/view52"
            app:layout_constraintTop_toBottomOf="@+id/view52" />

        <View
            android:id="@+id/view72"
            android:layout_width="@dimen/margin_30dp"
            android:layout_height="@dimen/margin_10dp"
            android:layout_marginTop="@dimen/margin_8dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toStartOf="@+id/view62"
            app:layout_constraintTop_toBottomOf="@+id/view62" />

        <View
            android:id="@+id/view82"
            android:layout_width="90dp"
            android:layout_height="@dimen/margin_10dp"
            android:layout_marginTop="@dimen/margin_8dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toStartOf="@+id/view72"
            app:layout_constraintTop_toBottomOf="@+id/view72" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/margin_12dp"
        android:visibility="gone"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

        <View
            android:id="@+id/view23"
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_1dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintTop_toTopOf="0" />

        <View
            android:id="@+id/view33"
            android:layout_width="54dp"
            android:layout_height="54dp"
            android:layout_marginTop="@dimen/margin_12dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toStartOf="0"
            app:layout_constraintTop_toBottomOf="@+id/view23" />

        <View
            android:id="@+id/view43"
            android:layout_width="@dimen/margin_30dp"
            android:layout_height="@dimen/margin_10dp"
            android:layout_marginLeft="@dimen/size_22dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toEndOf="@+id/view33"
            app:layout_constraintTop_toTopOf="@+id/view33" />

        <View
            android:id="@+id/view53"
            android:layout_width="@dimen/size_100dp"
            android:layout_height="@dimen/margin_10dp"
            android:layout_marginTop="@dimen/margin_8dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toStartOf="@+id/view43"
            app:layout_constraintTop_toBottomOf="@+id/view43" />

        <View
            android:id="@+id/view63"
            android:layout_width="@dimen/margin_30dp"
            android:layout_height="@dimen/margin_10dp"
            android:layout_marginTop="@dimen/margin_8dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toStartOf="@+id/view53"
            app:layout_constraintTop_toBottomOf="@+id/view53" />

        <View
            android:id="@+id/view73"
            android:layout_width="@dimen/margin_30dp"
            android:layout_height="@dimen/margin_10dp"
            android:layout_marginTop="@dimen/margin_8dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toStartOf="@+id/view63"
            app:layout_constraintTop_toBottomOf="@+id/view63" />

        <View
            android:id="@+id/view83"
            android:layout_width="90dp"
            android:layout_height="@dimen/margin_10dp"
            android:layout_marginTop="@dimen/margin_8dp"
            android:background="@color/greye0e6e9"
            app:layout_constraintStart_toStartOf="@+id/view73"
            app:layout_constraintTop_toBottomOf="@+id/view73" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
