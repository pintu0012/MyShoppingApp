<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/pwe_pressed_effect_color"/>
            <stroke android:width="1dp" android:color="@color/pwe_pressed_effect_color"/>
            <corners android:topLeftRadius="0dp" android:topRightRadius="8dp" android:bottomLeftRadius="8dp" android:bottomRightRadius="0dp"/>
            <padding android:left="10dp" android:top="10dp" android:right="10dp" android:bottom="10dp"/>
        </shape>
    </item>
    <item>
        <shape/>
    </item>
</selector>
