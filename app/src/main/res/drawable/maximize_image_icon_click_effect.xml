<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/pwe_pressed_effect_color"/>
            <stroke android:width="0.5dp" android:color="@color/pwe_border_color"/>
            <corners android:topLeftRadius="0dp" android:topRightRadius="6dp" android:bottomLeftRadius="6dp" android:bottomRightRadius="0dp"/>
            <padding android:left="2dp" android:top="2dp" android:right="2dp" android:bottom="2dp"/>
        </shape>
    </item>
    <item>
        <shape/>
    </item>
</selector>
